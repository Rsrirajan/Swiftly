{
    "manifest_version": 3,
    "name": "LaxApply",
    "version": "1.0.0",
    "permissions": ["scripting"],
    "host_permissions": [
        "https://*.joinhandshake.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://*.joinhandshake.com/*"],
            "js": ["Script.js"]
        }
    ],
    "icons": {
        "128": "128.png"
    }
}
